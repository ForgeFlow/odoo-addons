<odoo>
    <record id="view_l10n_es_aeat_report_form" model="ir.ui.view">
        <field name="model">l10n.es.aeat.mod303.report</field>
        <field name="inherit_id" ref="l10n_es_aeat.view_l10n_es_aeat_report_form"/>
        <field name="arch" type="xml">
            <xpath expr="//header" position="inside">
                <button name="button_first_prorrate_regularitzation" type="object" string="1st prorrate regularization"
